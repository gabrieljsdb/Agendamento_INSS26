{
    "name": "oab-sc/sistema-agendamento-inss",
    "description": "Sistema de Agendamento INSS - OAB/SC (VersÃ£o Melhorada)",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-soap": "*",
        "ext-json": "*",
        "vlucas/phpdotenv": "^5.5",
        "phpmailer/phpmailer": "^6.8",
        "monolog/monolog": "^3.3",
        "respect/validation": "^2.2",
        "twig/twig": "^3.5",
        "symfony/http-foundation": "^6.2",
        "robmorgan/phinx": "^0.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
